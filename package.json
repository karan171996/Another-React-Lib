{
  "name": "another-react-lib",
  "version": "1.3.0",
  "description": "another-react-lib React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "doc": "react-doc-generator ./src -o ./docs/documentation.md",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "passport": "^0.4.0"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.15.1",
    "nwb": "^0.23.0",
    "react": "^16.9.0",
    "react-doc-generator": "^1.2.5",
    "react-dom": "^16.9.0"
  },
  "author": "Nehemiah K",
  "homepage": "https://github.com/NehemiahK/Another-React-Lib/blob/master/README.md",
  "license": "MIT",
  "repository": "github:NehemiahK/Another-React-Lib",
  "keywords": [
    "react-component"
  ]
}
